<div class="panel panel-primary">
	<div class="panel-heading">Create New BillingCycle</div>
	<div class="panel-body">

<div class="form form-horizontal form-view">
	<div class="form-group">
		<label class="control-label col-md-2 required" for="service_id" id="service_id-label">Service</label>
		<div class='col-md-8'>{{view App.Select2 contentBinding='service_id_array' valueBinding='service' optionLabelPath='content.id' class='form-control'}}</div>	</div>
	<div class="form-group">
		<label class="control-label col-md-2 required" for="cycle" id="cycle-label">Cycle</label>
		<div class="col-md-6">{{input id="cycle" class="form-control type-int" value=cycle}}</div>
	</div>
	<div class="form-group">
		<label class="control-label col-md-2 required" for="discount" id="discount-label">Discount</label>
		<div class="col-md-6">{{input id="discount" class="form-control type-int" value=discount}}</div>
	</div>
	<div class="form-group">
		<label class="control-label col-md-2 " for="created_at" id="created_at-label">Created At</label>
		<div class="col-md-6">{{input id="created_at" class="form-control type-timestamp" value=created_at}}</div>
	</div>
	<div class="form-group">
		<label class="control-label col-md-2 " for="updated_at" id="updated_at-label">Updated At</label>
		<div class="col-md-6">{{input id="updated_at" class="form-control type-timestamp" value=updated_at}}</div>
	</div>
	<div class="col-md-offset-2">
		<button class="btn btn-primary" {{action 'save'}}>Save</button>
		<button class="btn btn-default" {{action 'cancel'}}>Cancel</button>
	</div>
</div>

	</div>
</div>